<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blues.lin.mpauto.mapper.UserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.blues.lin.mpauto.entity.UserInfo">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="account" property="account" />
        <result column="password" property="password" />
        <result column="email" property="email" />
        <result column="age" property="age" />
    </resultMap>

    <select id="selectPageVo" resultType="com.blues.lin.mpauto.entity.UserInfo">
        SELECT * FROM mybatis_user_info
    </select>

</mapper>
